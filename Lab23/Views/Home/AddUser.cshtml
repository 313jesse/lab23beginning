
@{
    ViewBag.Title = "AddUser";
}



<h2>AddUser</h2>
<form action="/Home/AddUser" method="post">
    <input type="hidden" name="AddUser" value="@ViewBag.AddUser." /> ////?
    <label>User Name:</label><input type="text" name="UserName" class="form-control" value="@ViewBag.AddUser.UserName" />  @*//do not want to show this part to users*@
    <label>User ID:</label> <input type="text" name="UserID" class="form-control" value="@ViewBag.AddUser.UserID" />
    <label>Address:</label> <input type="text" name="Address" class="form-control" value="@ViewBag.AddUser.Address" />
    <label>Registration Date:</label> <input type="date" name="RegistrationDate" class="form-control" value="@ViewBag.AddUser.RegisterDate" />
    <input type="submit" value="Save!" class="form-control" />
</form>


